<?xml
    version="1.0"
    encoding="utf-8"?>

<resources>

    <declare-styleable  name="roundedimageview">
        <attr
            name="border_thickness"
            format="dimension"
            />

        <attr
            name="border_inside_color"
            format="color"
            />
        <attr
            name="border_outside_color"
            format="color"></attr>

    </declare-styleable>
    <declare-styleable name="PraiseListView">
        <attr name="item_color" format="color"/>
        <attr name="item_selector_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ExpandTextView">
        <attr name="showLines" format="integer"/>
    </declare-styleable>
</resources>